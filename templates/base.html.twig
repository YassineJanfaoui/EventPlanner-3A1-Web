    <style>
    /* ===== Base Styles ===== */
html {
  scroll-behavior: smooth;
}

body {
  font-family: 'Poppins', sans-serif;
  color: #2c234d;
  line-height: 1.7;
  background: #fff;
  overflow-x: hidden;
}

.no-js body {
  padding-top: 3.125rem;
}

a {
  text-decoration: none;
  color: inherit;
  transition: all 0.3s ease;
}

a:hover {
  color: #fb246a;
}

img {
  max-width: 100%;
  height: auto;
}

.btn {
  display: inline-block;
  padding: 12px 30px;
  border-radius: 30px;
  font-weight: 600;
  text-transform: uppercase;
  transition: all 0.3s ease;
}

.header-btn {
  background: #fb246a;
  color: #fff !important;
}

.header-btn:hover {
  background: #2c234d;
  color: #fff !important;
}

/* ===== Header Styles ===== */
.header-area {
  width: 100%;
  position: fixed;
  z-index: 9999;
  background: #fff;
  box-shadow: 0 0 10px rgba(0,0,0,0.1);
}

.header-sticky {
  padding: 15px 0;
  transition: all 0.3s ease;
}

.main-header {
  padding: 15px 0;
}

.logo img {
  max-height: 50px;
}

.main-menu ul {
  margin: 0;
  padding: 0;
  list-style: none;
}

.main-menu > nav > ul > li {
  display: inline-block;
  position: relative;
  margin: 0 15px;
}

.main-menu > nav > ul > li > a {
  color: #2c234d;
  font-weight: 500;
  padding: 10px 0;
  display: block;
}

.main-menu > nav > ul > li:hover > a {
  color: #fb246a;
}

/* Submenu Styles */
.submenu {
  position: absolute;
  width: 200px;
  background: #fff;
  box-shadow: 0 0 10px rgba(0,0,0,0.1);
  padding: 15px 0;
  top: 100%;
  left: 0;
  opacity: 0;
  visibility: hidden;
  transition: all 0.3s ease;
}

.main-menu > nav > ul > li:hover .submenu {
  opacity: 1;
  visibility: visible;
}

.submenu li {
  padding: 0 20px;
}

.submenu li a {
  display: block;
  padding: 8px 0;
  color: #2c234d;
}

.submenu li a:hover {
  color: #fb246a;
}

/* Mobile Menu */
.mobile_menu {
  display: none;
}

/* ===== Footer Styles ===== */
.footer-bg {
  background: #040E27;
  padding: 70px 0 20px;
}

.footer-border {
  border-top: 1px solid rgba(255,255,255,0.1);
  padding-top: 20px;
}

.footer-copy-right p {
  color: #fff;
  margin-bottom: 0;
}

.footer-social a {
  color: #fff;
  font-size: 20px;
  margin-left: 15px;
  transition: all 0.3s ease;
}

.footer-social a:hover {
  color: #fb246a;
}

/* Back to Top Button */
#back-top {
  position: fixed;
  right: 30px;
  bottom: 30px;
  z-index: 99;
  display: none;
}

#back-top a {
  width: 50px;
  height: 50px;
  background: #fb246a;
  color: #fff;
  display: block;
  text-align: center;
  line-height: 50px;
  border-radius: 50%;
  transition: all 0.3s ease;
}

#back-top a:hover {
  background: #2c234d;
}

/* ===== Responsive Styles ===== */
@media (max-width: 991px) {
  .main-menu {
    display: none;
  }
  
  .mobile_menu {
    display: block;
  }
  
  .header-right-btn {
    display: none;
  }
  
  .logo {
    text-align: center;
  }
}

@media (max-width: 767px) {
  .footer-copy-right {
    text-align: center;
    margin-bottom: 20px;
  }
  
  .footer-social {
    text-align: center;
    float: none !important;
  }
  
  .footer-social a {
    margin: 0 8px;
  }
}

/* ===== Animation Styles ===== */
.wow {
  visibility: hidden;
}

.animated {
  animation-duration: 1s;
  animation-fill-mode: both;
}

/* ===== Utility Classes ===== */
.d-none {
  display: none !important;
}

.d-block {
  display: block !important;
}

.d-lg-none {
  display: none !important;
}

@media (min-width: 992px) {
  .d-lg-block {
    display: block !important;
  }
  
  .d-lg-none {
    display: none !important;
  }
}

.ml-30 {
  margin-left: 30px;
}

.f-right {
  float: right;
}

/* ===== Form Styles ===== */
.nice-select {
  width: 100%;
  border-radius: 5px;
  border: 1px solid #eee;
  height: 50px;
  line-height: 50px;
}

/* ===== Plugin Overrides ===== */
.slicknav_menu {
  display: none;
  background: transparent;
  padding: 0;
}

@media (max-width: 991px) {
  .slicknav_menu {
    display: block;
  }
}

.slicknav_btn {
  background: #fb246a;
  margin: 15px 0;
}

.slicknav_nav {
  background: #fff;
  box-shadow: 0 0 10px rgba(0,0,0,0.1);
}

.slicknav_nav a {
  color: #2c234d;
  padding: 10px 20px;
}

.slicknav_nav a:hover {
  background: #fb246a;
  color: #fff;
}

.slicknav_nav .slicknav_row:hover {
  background: #fb246a;
  color: #fff;
}

.slicknav_nav .submenu {
  display: none;
  padding-left: 20px;
}

.slicknav_nav .slicknav_item:hover .submenu {
  display: block;
}
    </style>
</head>
