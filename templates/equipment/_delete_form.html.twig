<form style="display: inline-block" method="post" action="{{ path('app_equipment_delete', {'EquipmentId': equipment.EquipmentId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ equipment.EquipmentId) }}">
    <button class="btn btn-primary" style="background-color:darkred">Delete</button>
</form>
