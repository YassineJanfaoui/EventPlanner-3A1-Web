{% extends 'back.html.twig' %}

{% block title %}Partner
{% endblock %}

{% block body %}
	<div class="card mb-4">
		<div class="card-header">Partner</div>
		<div class="card-body">
			<table class="table">
				<tbody>
					<tr>
						<th>PartnerId</th>
						<td>{{ partner.partnerId }}</td>
					</tr>
					<tr>
						<th>Name</th>
						<td>{{ partner.name }}</td>
					</tr>
					<tr>
						<th>Category</th>
						<td>{{ partner.category }}</td>
					</tr>
					<tr>
						<th>Email</th>
						<td>{{ partner.email }}</td>
					</tr>
					<tr>
						<th>Number</th>
						<td>{{ partner.number }}</td>
					</tr>
					<tr>
						<th>OfficeLocation</th>
						<td>{{ partner.officeLocation }}</td>
					</tr>
				</tbody>
			</table>

			<a href="{{ path('app_admin_partner_index') }}" class="btn btn-primary " style="width:25%; margin-left:37.5%; display:block;">back to list</a>

			{{ include('partner/_delete_form.html.twig') }}
		</div>
	</div>
{% endblock %}
